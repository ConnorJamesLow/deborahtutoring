{
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules/*"
    ],
    "compilerOptions": {
        "target": "es2016",
        "lib": [
            "ES2019",
			"dom"
        ],
        "module": "esnext",
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "types": [
            "svelte",
            "node",
            "@sapper",
            "site"
        ],
        "typeRoots": [
            "types"
        ]
    }
}